# Phil template for experiment manager
dry_run = {dry_run}
input {{
  cfg = {cfg}
  experiment = {experiment}
  run_num = {run_num}
  trial = {trial}
  rungroup = {rungroup}
  xtc_dir = None
  dispatcher = {dispatcher}
  target = {target}
}}
output {{
  output_dir = {output_dir}
  split_logs = True
}}
mp {{
  method = *mpi sge pbs custom
  nproc = {nproc}
  queue = {queue}
}}
experiment_tag = {experiment_tag}
db {{
  host = {host}
  name = {dbname}
  user = {user}
  password = {password}
}}
