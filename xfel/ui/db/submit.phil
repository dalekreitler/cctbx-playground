# Phil template for experiment manager
dry_run = {dry_run}
input {{
  experiment = {experiment}
  run_num = {run_num}
  trial = {trial}
  rungroup = {rungroup}
  xtc_dir = None
  dispatcher = cctbx.xfel.xtc_process
  target = None
}}
output {{
  output_dir = {output_dir}
  split_logs = True
}}
mp {{
  method = *mpi sge pbs custom
  nproc = {nproc}
  queue = {queue}
}}
